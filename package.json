{
  "name": "ray-restaurant-website",
  "version": "1.3.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "validate:fonts": "tsx scripts/font-validation.ts",
    "validate:sitemap": "tsx scripts/validate-sitemap.ts",
    "validate:all": "npm run validate:fonts && npm run validate:sitemap",
    "sitemap:generate": "tsx scripts/generate-sitemap.ts",
    "sitemap:validate": "npm run sitemap:generate && npm run validate:sitemap",
    "analyze:bundle": "tsx scripts/analyze-bundle.ts",
    "analyze:next": "ANALYZE=true npm run build",
    "optimize:advanced": "tsx scripts/advanced-performance.ts",
    "optimize:all": "npm run optimize:advanced && npm run build"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.2",
    "lucide-react": "^0.542.0",
    "negotiator": "^1.0.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.4",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3"
  }
}
